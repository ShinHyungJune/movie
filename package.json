      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "json-server": "^0.15.1"
  },
  "proxy": "http://localhost:5000"
