  "dependencies": {
    "cross-env": "^6.0.3",
    "materialize-css": "^1.0.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-moment": "^0.9.2",
    "react-redux": "^7.0.3",
    "react-router": "^5.1.2",
